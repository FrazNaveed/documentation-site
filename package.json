{
  "name": "flare-payload",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "next": "cross-env NODE_OPTIONS=--no-deprecation next",
    "build": "cross-env next build",
