{
  "name": "flare-payload",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "docker:down": "docker-compose -f docker-compose-postgres.dev.yml down && docker-compose -f docker-compose-next.dev.yml down",
    "docker:down:next": "docker-compose -f docker-compose-next.dev.yml down",
    "docker:down:pg": "docker-compose -f docker-compose-postgres.dev.yml down",
    "docker:init": "./scripts/dockerInit.sh",
    "docker:up": "npm run docker:down && docker-compose -f docker-compose-postgres.dev.yml up -d && docker-compose -f docker-compose-next.dev.yml up",
    "docker:up:next": "docker-compose -f docker-compose-next.dev.yml up",
    "docker:up:pg": "docker-compose -f docker-compose-postgres.dev.yml up",
    "generate:importmap": "cross-env payload generate:importmap",
    "generate:types": "cross-env payload generate:types",
    "lint": "cross-env next lint",
    "lint:fix": "cross-env next lint --fix",
    "lint:styles": "stylelint \"**/*.{css,sass,scss}\"",
    "migrate:create": "npm run payload migrate:create",
    "payload": "cross-env PAYLOAD_CONFIG_PATH=./payload.config.ts payload",
    "start": "cross-env next start",
    "prepare": "husky",
    "seed:news": "node 'scripts/seedNews.js'",
    "seed:news-de": "node 'scripts/seedNewsDe.js'",
    "seed:news-es": "node 'scripts/seedNewsEs.js'",
    "seed:news-types": "node 'scripts/seedNewsTypes.js'",
    "seed:news-types-de": "node 'scripts/seedNewsTypesDe.js'",
    "seed:news-types-es": "node 'scripts/seedNewsTypesEs.js'"
  },
  "dependencies": {
    "@faker-js/faker": "^9.0.1",
    "@formatjs/intl-localematcher": "^0.5.4",
    "@next/bundle-analyzer": "^14.2.11",
    "@payloadcms/db-postgres": "3.0.0-beta.109",
    "@payloadcms/next": "3.0.0-beta.109",
    "@payloadcms/plugin-cloud": "3.0.0-beta.109",
    "@payloadcms/richtext-lexical": "3.0.0-beta.109",
    "@payloadcms/storage-vercel-blob": "3.0.0-beta.109",
    "axios": "^1.7.7",
    "classnames": "^2.5.1",
    "cross-env": "^7.0.3",
    "graphql": "^16.8.1",
    "lodash.throttle": "^4.1.1",
    "negotiator": "^0.6.3",
    "next": "15.0.0-canary.160",
    "payload": "3.0.0-beta.109",
    "react": "19.0.0-rc-67fee58b-20240926",
    "react-dom": "19.0.0-rc-67fee58b-20240926",
    "react-remove-scroll": "^2.6.0",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@aleph/eslint-config-nought": "^1.1.10",
    "@aleph/nought-sass-mixins": "^2.0.0",
    "@next/eslint-plugin-next": "^15.0.0-canary.104",
    "@types/lodash.throttle": "^4.1.9",
    "@types/negotiator": "^0.6.3",
    "@types/node": "22.5.4",
    "@types/react": "npm:types-react@19.0.0-rc.0",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.0",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "eslint": "^8",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "15.0.0-canary.104",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.36.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.45",
    "prettier": "^3.3.3",
    "sass": "^1.78.0",
    "stylelint": "^16.9.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.2",
    "stylelint-scss": "^6.5.1",
    "typescript": "5.6.2"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "npm:types-react@19.0.0-rc.0",
      "@types/react-dom": "npm:types-react-dom@19.0.0-rc.0"
    }
  },
  "overrides": {
    "@types/react": "npm:types-react@19.0.0-rc.0",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.0"
  }
}
