{
  "name": "flare-payload",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env next build",
    "db:down": "docker compose down postgres",
    "db:import": "docker compose exec -T postgres psql -U postgres -d flarepayload < dump.sql",
    "db:up": "docker compose up postgres",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "docker:init": "npm run docker:clean && docker compose up",
    "docker:clean": "docker compose down --rmi all -v",
    "generate:importmap": "cross-env payload generate:importmap",
    "generate:types": "cross-env payload generate:types",
    "lint": "cross-env next lint",
    "lint:fix": "cross-env next lint --fix",
    "lint:styles": "stylelint \"**/*.{css,sass,scss}\"",
    "payload": "cross-env PAYLOAD_CONFIG_PATH=./payload.config.ts payload",
    "start": "cross-env next start",
    "prepare": "husky"
  },
  "dependencies": {
    "@faker-js/faker": "^9.0.1",
    "@formatjs/intl-localematcher": "^0.5.4",
    "@formspree/react": "^2.5.1",
    "@next/bundle-analyzer": "^14.2.11",
    "@payloadcms/db-postgres": "^3.5.0",
    "@payloadcms/email-nodemailer": "^3.5.0",
    "@payloadcms/next": "^3.5.0",
    "@payloadcms/payload-cloud": "^3.5.0",
    "@payloadcms/plugin-seo": "^3.5.0",
    "@payloadcms/richtext-lexical": "^3.5.0",
    "@payloadcms/storage-vercel-blob": "^3.5.0",
    "@radix-ui/react-accordion": "^1.2.2",
    "@radix-ui/react-portal": "^1.1.3",
    "@radix-ui/react-scroll-area": "^1.2.2",
    "axios": "^1.7.7",
    "classnames": "^2.5.1",
    "content-collections": "^0.1.3",
    "country-flag-icons": "^1.5.13",
    "cross-env": "^7.0.3",
    "graphql": "^16.8.1",
    "gray-matter": "^4.0.3",
    "lodash": "^4.17.21",
    "lodash.throttle": "^4.1.1",
    "lucide-react": "^0.473.0",
    "negotiator": "^0.6.3",
    "next": "^15.0.4",
    "next-mdx-remote": "^5.0.0",
    "payload": "^3.5.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-player": "^2.16.0",
    "react-remove-scroll": "^2.6.0",
    "react-share": "^5.1.1",
    "sharp": "^0.33.5",
    "slugify": "^1.6.6",
    "swiper": "^11.1.14"
  },
  "devDependencies": {
    "@aleph/eslint-config-nought": "^1.1.10",
    "@aleph/nought-sass-mixins": "^2.0.0",
    "@content-collections/core": "0.8.0",
    "@content-collections/mdx": "0.2.0",
    "@content-collections/next": "0.2.4",
    "@next/eslint-plugin-next": "^15.0.4",
    "@shikijs/rehype": "2.1.0",
    "@types/lodash.throttle": "^4.1.9",
    "@types/negotiator": "^0.6.3",
    "@types/node": "22.5.4",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "eslint": "^8",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^15.0.4",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.36.1",
    "eslint-plugin-react-hooks": "^5.1.0",
    "github-slugger": "2.0.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.45",
    "prettier": "^3.3.3",
    "rehype-slug": "6.0.0",
    "remark-gfm": "4.0.0",
    "sass": "^1.78.0",
    "shiki": "^2.1.0",
    "stylelint": "^16.9.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.2",
    "stylelint-scss": "^6.5.1",
    "typescript": "5.6.2"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  }
}
